{
  "name": "astylarui",
  "version": "0.1.0",
  "description": "A 3D UI rendering library that renders HTML-like structures in BabylonJS scenes",
  "main": "dist/lib/lib/index.js",
  "module": "dist/lib/lib/index.js",
  "types": "dist/lib/lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/lib/index.d.ts",
      "import": "./dist/lib/lib/index.js",
      "default": "./dist/lib/lib/index.js"
    }
  },
  "files": [
    "dist/lib",
    "README.md"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "if exist dist\\lib rmdir /s /q dist\\lib && tsc -p tsconfig.lib.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:astylarui": "node dist/astylarui/server/server.mjs",
    "prepublishOnly": "npm run build:lib",
    "build:gh-pages": "ng build --base-href /AstylarUI/ --configuration production",
    "deploy": "npm run build:gh-pages && npx angular-cli-ghpages --dir=dist/astylarui/browser"
  },
  "keywords": [
    "babylonjs",
    "3d",
    "ui",
    "rendering",
    "angular",
    "webgl"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/the-erin-collective/AstylarUI"
  },
  "license": "AGPL-3.0",
  "peerDependencies": {
    "@angular/core": "^20.0.0",
    "@babylonjs/core": "^8.0.0"
  },
  "dependencies": {
    "@angular/cdk": "^20.0.5",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.0.5",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/ssr": "^20.0.3",
    "@babylonjs/core": "^8.0.0",
    "@babylonjs/loaders": "^8.15.1",
    "@ngrx/operators": "^20.0.0-beta.0",
    "@ngrx/signals": "^20.0.0-beta.0",
    "express": "^5.1.0",
    "round-polygon": "^0.6.7",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.3",
    "@angular/cli": "^20.0.3",
    "@angular/compiler-cli": "^20.0.0",
    "@types/babylonjs": "^2.3.31",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "angular-cli-ghpages": "^2.0.3",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2"
  }
}